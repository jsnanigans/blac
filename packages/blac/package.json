{
  "name": "blac",
  "version": "1.0.9",
  "license": "MIT",
  "main": "src/index.ts",
  "module": "src/index.ts",
  "typings": "src/index.ts",
  "types": "src/index.ts",
  "keywords": [
    "react",
    "typescript",
    "rxjs",
    "state-management",
    "reactjs",
    "observer-pattern",
    "bloc",
    "bloc-pattern"
  ],
  "scripts": {
    "build": "vite build",
    "pub:patch": "npm run build && bun publish.ts && cd dist && npm publish",
